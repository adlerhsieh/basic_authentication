<h3>Sign In</h3>
<hr/>
<%= form_tag sessions_path do %>
	<p>Name: <%= text_field_tag :name %></p>
	<p>Password: <%= password_field_tag :password %></p>
	<p><%= submit_tag "Sign in" %></p>
<% end %>
<% if flash.any? %>
	<hr/>
	<% flash.each do |key, value| %>
		<p><%= value %></p>
	<% end %>
<% end %>
<hr/>
<%= link_to "Cancel", users_path %>